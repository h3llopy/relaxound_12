<odoo>
    <data noupdate="1">

        <record model="ir.cron" id="do_export_file_cron_out_inovices">
            <field name="name">DATEV Automatic Export of Outgoing Invoices/Refunds</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="model_id" ref="model_datev_export_options_automate"/>
            <field name="function" eval="'do_export_file_cron'"/>
            <field name="args" eval="'(True, True, False, False)'"/>
        </record>

        <record model="ir.cron" id="do_export_file_cron_in_invoices">
            <field name="name">DATEV Automatic Export of Incoming Invoices/Refunds</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <!-- <field name="doall" eval="False"/> -->
            <!-- <field name="model" eval="'datev_export'"/> -->
            <field name="model_id" ref="model_datev_export_options_automate"/>
            <field name="function" eval="'do_export_file_cron'"/>
            <field name="args" eval="'(False, False, True, True)'"/>
            <!-- <field name="code">model.do_export_file_cron()</field> -->
            <field name="state">code</field>
        </record>


    </data>
</odoo>
