<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="wp_odoo_service_messanger_form_view" model="ir.ui.view">
            <field name="name">service.messanger.woo.form</field>
            <field name="model">service.messanger</field>
            <field name="inherit_id" ref="service_messanger.view_service_messanger_form" />
            <field eval="7" name="priority"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet" position="before">
                   <header position="inside">
                    <button name="sync_service_messanger" string="Sync" type="object" class="oe_highlight"/>
                </header>
                </xpath>
                
                <xpath expr="//field[@name='customer_id']" position="after">
                    <field name="backend_id" widget="many2many_tags" required="1"/>
                    
                </xpath>
            </field>
        </record>
    </data>
</odoo>