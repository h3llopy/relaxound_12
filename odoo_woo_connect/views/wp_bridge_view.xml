		<menuitem action="wp_odoo_site_action" groups="group_wp_odoo_user" id="wp_odoo_id0" name="WP Site" parent="menu_wp_odoo_mapping_id"/>
		<menuitem groups="group_wp_odoo_user" id="menu_wp_odoo_mapping_id" name="Mapping" parent="menu_point_root" sequence="1"/>
